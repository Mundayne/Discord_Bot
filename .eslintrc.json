{
	"extends": "standard",
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"indent": ["error", "tab"],
		"no-console": "error",
		"no-tabs": "off",
		"no-shadow": ["error", { "builtinGlobals": true }],
		"no-shadow-restricted-names": "error",
		"no-warning-comments": "warn",
		"no-var": "error",
		"prefer-const": "off"
	}
}
